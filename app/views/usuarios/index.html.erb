<p id="notice"><%= notice %></p>

<h1>Listing Usuarios</h1>

<table>
  <thead>
    <tr>
      <th>Codigo usuario</th>
      <th>Nickname</th>
      <th>Password</th>
      <th>Nombres usuario</th>
      <th>Apellidos usuario</th>
      <th>Correo usuario</th>
      <th>Telefono usuario</th>
      <th>Estado usuario</th>
      <th>Tipo usuario</th>
      <th>Fecha creacion usuario</th>
      <th>Fecha actualizacion usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @usuarios.each do |usuario| %>
      <tr>
        <td><%= usuario.Codigo_Usuario %></td>
        <td><%= usuario.Nickname %></td>
        <td><%= usuario.Password %></td>
        <td><%= usuario.Nombres_usuario %></td>
        <td><%= usuario.Apellidos_usuario %></td>
        <td><%= usuario.Correo_usuario %></td>
        <td><%= usuario.Telefono_usuario %></td>
        <td><%= usuario.Estado_usuario %></td>
        <td><%= usuario.Tipo_usuario %></td>
        <td><%= usuario.Fecha_Creacion_usuario %></td>
        <td><%= usuario.Fecha_Actualizacion_usuario %></td>
        <td><%= link_to 'Show', usuario %></td>
        <td><%= link_to 'Edit', edit_usuario_path(usuario) %></td>
        <td><%= link_to 'Destroy', usuario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Usuario', new_usuario_path %>
