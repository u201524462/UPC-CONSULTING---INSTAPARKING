<%= simple_form_for(@automovil) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :Placa_auto %>
    <%= f.input :Marca_auto %>
    <%= f.input :Modelo_auto %>
    <%= f.input :Color_auto %>
    <%= f.input :Fecha_Registro_auto %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
